      "patch": {
        "summary": "Update asset",
        "tags": [
          "Assets"
        ],
        "description": "Update an asset.\n\nThe user updating the asset must have at least super manager role on the site the asset belongs to.\n",
        "parameters": [
          {
            "name": "id",
            "description": "ID of the asset.",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "Asset properties",
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "properties": {
                  "upload_id": {
