                    "example": "4e12729c-96a8-436b-ba63-4d5cfc92abf3"
                  },
                  "type": {
                    "type": "string",
                    "description": "Type of the asset. Can be one of `illustration` or `audio`.",
                    "example": "illustration"
                  }
                },
                "required": [
                  "upload_id",
                  "name",
                  "site_id",
                  "type"
                ]
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "properties": {
