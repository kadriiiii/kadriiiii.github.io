              "application/vnd.salemove.v1+json": {
                "schema": {
                  "type": "object",
                  "description": "Applet layout",
                  "properties": {
                    "id": {
                      "type": "string",
                      "example": "8a70a9c5-d8ff-4e3f-afd8-cdae91bb0e79",
                      "description": "The ID of the applet layout."
                    },
                    "site_id": {
                      "description": "The ID of the site that owns the applet layout. Users with super manager role on the site have full access to\nthis layout.\n",
                      "type": "string"
