    "/axons": {
      "post": {
        "summary": "Create applet",
        "tags": [
          "Applet"
        ],
        "description": "Creates a new applet for the site with the ID `site_id`.\n\nThe user making the request must have the super manager role on the given site.\n",
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "owner_site_id",
                  "name",
                  "source"
                ],
                "type": "object",
                "properties": {
                  "name": {
                    "description": "Name of the applet.",
                    "type": "string"
                  },
                  "description": {
                    "description": "Short description of the applet.",
                    "type": "string"
                  },
                  "assign_to_site": {
                    "type": "boolean",
                    "description": "Assigns the created applet to the `owner_site_id`.",
                    "default": false
                  },
                  "owner_site_id": {
                    "description": "The ID of the site that owns the applet. Users with super manager role on the site have full access to this applet.\n",
                    "type": "string"
                  },
                  "source": {
                    "type": "string",
                    "format": "binary",
                    "description": "Applet source code. Must be HTML."
                  },
                  "icon": {
                    "type": "string",
                    "format": "binary",
                    "description": "Applet icon. Must be one of `png`, `svg`."
                  }
                }
              },
              "encoding": {
                "source": {
                  "contentType": "text/html"
                },
                "icon": {
                  "contentType": "image/png, image/jpeg"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Applet successfully created.",
