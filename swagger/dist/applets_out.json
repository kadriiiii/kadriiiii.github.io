      "post": {
        "summary": "Create layout",
        "tags": [
          "Applet layout"
        ],
        "description": "Creates a new applet layout for the site with the ID `site_id`.\n\nThe user making the request must have the super manager role on the given site.\n",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "required": [
                  "site_id",
                  "name"
                ],
                "type": "object",
                "properties": {
                  "site_id": {
                    "description": "The ID of the site that owns the applet layout. Users with super manager role on the site have full access to\nthis layout.\n",
                    "type": "string"
                  },
                  "name": {
                    "description": "Name of the applet layout.",
                    "type": "string"
                  },
                  "description": {
                    "description": "Short description of the applet layout.",
                    "type": "string"
                  },
                  "is_default": {
                    "description": "Default layout used when no specific applet layout is found with other conditions. `false` by default. Only one\nper site can be `true`. Setting default will change other record to not default.\n",
                    "type": "boolean",
                    "default": false,
                    "enum": [
                      true,
                      false
                    ]
                  },
                  "axon_ids": {
                    "description": "Ordered list of applets to display in this layout.",
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  },
                  "queue_ids": {
                    "description": "Ordered list of queues that this applet layout applies to.",
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Layout successfully created.",
